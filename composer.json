{
    "name": "developifynet/brightpearl-php",
    "description": "Brightpearl API Wrapper for PHP",
    "license": "MIT",
    "keywords": [
        "brightpearl",
        "api",
        "php",
        "sdk"
    ],
    "authors": [
        {
            "name": "Mustafa Mughal",
            "email": "efficientbit@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-0": {
            "Developifynet\\Brightpearl": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Developifynet\\Brightpearl\\BrightpearlServiceProvider"
            ],
            "aliases": {
                "Brightpearl": "Developifynet\\Brightpearl\\Brightpearl"
            }
        }
    }
}
